{
  "name": "btp-cap-tutorial",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^7",
    "@sap/cds-hana": "^2",
    "@sap/cds-sqlite": "^1.24.1",
    "express": "^4",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "devDependencies": {
    "@cap-js/cds-typer": ">=0.1",
    "@cap-js/sqlite": "^1.7.2",
    "@sap/eslint-plugin-cds": "^3.0.4",
    "eslint": "^9"
  },
  "scripts": {
    "build": "tsc && cp -r @cds-models dist",
    "start": "cds-ts serve",
    "watch": "cds-ts watch",
    "test": "jest"
  },
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  },
  "cds": {
    "requires": {
      "db": {
        "[development]": {
          "kind": "sqlite",
          "impl": "@cap-js/sqlite",
          "credentials": {
            "url": "memory"
          }
        },
        "[production]": {
          "kind": "hana",
          "impl": "@sap/cds-hana",
          "deploy-format": "hdbtable"
        }
      }
    }
  }
}

